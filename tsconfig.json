{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": false,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./*"],
      "#/*": ["./*"],
      "#types": ["types/index"],
      "#stores/*": ["stores/*"],
      "#components/*": ["components/*"],
      "#assets/*": ["assets/*"]
    },
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.vue", "auto-imports.d.ts", "components.d.ts"]
}
